{
  "$schema": "https://angular.io/schemas/service-worker-config.schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/offline.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/icons/**",
          "/assets/**",
          "/**/*.png",
          "/**/*.svg"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "google-fonts-styles",
      "urls": [
        "https://fonts.googleapis.com/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "7d",
        "timeout": "3s"
      }
    },
    {
      "name": "google-fonts-webfonts",
      "urls": [
        "https://fonts.gstatic.com/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 30,
        "maxAge": "30d"
      }
    }
  ]
}